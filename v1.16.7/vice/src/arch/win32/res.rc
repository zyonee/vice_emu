#include "res.h"
#include "config.h"
#include "version.h"


1 VERSIONINFO
 FILEVERSION VERSION_RC_NUMBER
 PRODUCTVERSION VERSION_RC_NUMBER
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "04090000"
        BEGIN
            VALUE "CompanyName",     "Vice Team"
            VALUE "FileDescription", "Vice Emulator"
            VALUE "FileVersion",     VERSION
            VALUE "ProductName",     "Vice Emulator"
            VALUE "ProductVersion",  VERSION
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 0
    END
END


