// template of dialog for all languages

IDD_PETREU_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX  "", IDC_PETREU_ENABLE,
                  7, 7, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT         "", IDC_PETREU_SIZE_LABEL,
                  9, 24, 30, 8
    COMBOBOX      IDC_PETREU_SIZE,
                  45, 22, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_PETREU_FILE_LABEL,
                  9, 44, 30, 8
    PUSHBUTTON    "", IDC_PETREU_BROWSE,
                  45, 42, 50, 14, 0
    EDITTEXT      IDC_PETREU_FILE,
                  9, 58, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 90, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 90, 50, 14, WS_TABSTOP
END

