msgstr "Inaktivera emulatoridentifiering"

#: src/translate.c.po.c:3892
msgid "Enable the IEEE488 interface emulation"
msgstr "Aktivera emulering av IEEE488-gränssnitt"

#: src/translate.c.po.c:3906
msgid "Disable the IEEE488 interface emulation"
msgstr "Inaktivera emulering av IEEE488-gränssnitt"

#: src/translate.c.po.c:3920
msgid "<revision>"
msgstr "<utgåva>"

#: src/translate.c.po.c:3934
msgid "Patch the Kernal ROM to the specified <revision>"
msgstr "Patcha kernal-ROM till angiven <utgåva>"

#: src/translate.c.po.c:3949
msgid "Enable the $DE** ACIA RS232 interface emulation"
msgstr "Aktivera $DE** ACIA RS232-gränssnittsemulering"

#: src/translate.c.po.c:3963
msgid "Disable the $DE** ACIA RS232 interface emulation"
msgstr "Inaktivera $DE** ACIA-RS232-gränssnittsemulering"

#: src/translate.c.po.c:3993
msgid "Specify index of keymap file (0=symbol, 1=positional)"
msgstr ""
"Ange index för för tangentbordsinställningsfil (0=symbolisk, "
"1=positionsriktig)"

#: src/translate.c.po.c:4010
msgid "Specify name of symbolic keymap file"
msgstr "Ange fil för symbolisk tangentbordsemulering"

#: src/translate.c.po.c:4024
msgid "Specify name of positional keymap file"
msgstr "Ange fil för positionsriktig tangentbordsemulering"

#: src/translate.c.po.c:4039
msgid "Activate 40 column mode"
msgstr "Aktivera 40-kolumnersläge"

#: src/translate.c.po.c:4053
msgid "Activate 80 column mode"
msgstr "Aktivera 80-kolumnersläge"

#: src/translate.c.po.c:4067
msgid "Always switch to C64 mode on reset"
msgstr "Växla alltid till C64-läge vid återställning"

#: src/translate.c.po.c:4081
msgid "Always switch to C128 mode on reset"
msgstr "Växla alltid till C128-läge vid återställning"

#: src/translate.c.po.c:4095
msgid "Specify name of internal Function ROM image"
msgstr "Ange namn på intern funktions-ROM-avbildning"

#: src/translate.c.po.c:4109
msgid "Specify name of external Function ROM image"
msgstr "Ange namn på extern funktions-ROM-avbildning"

#: src/translate.c.po.c:4123
msgid "Enable the internal Function ROM"
msgstr "Aktivera internt funktions-ROM"

#: src/translate.c.po.c:4137
msgid "Disable the internal Function ROM"
msgstr "Inaktivera internt funktions-ROM"

#: src/translate.c.po.c:4151
msgid "Enable the external Function ROM"
msgstr "Aktivera externt funktions-ROM"

#: src/translate.c.po.c:4165
msgid "Disable the external Function ROM"
msgstr "Inaktivera externt funktions-ROM"

#: src/translate.c.po.c:4179
msgid "Use old NTSC sync factor"
msgstr "Använd gammal NTSC-synkfaktor"

#: src/translate.c.po.c:4193
msgid "Select Hummer Userport device (0: None, 1: ADC, 2: Joystick)"
msgstr "Välj enhet för Hummeranvändarport (0: ingen, 1: ADC, 2: styrspak)"

#: src/translate.c.po.c:4208
msgid "<port>"
msgstr "<port>"

#: src/translate.c.po.c:4222
msgid "Select which joystick port should be mapped to the userport"
msgstr "Ange vilken spelport som skall kopplas till användarporten"

#: src/translate.c.po.c:4236
msgid "Specify name of Kernal ROM image"
msgstr "Ange namn på kernal-ROM-avbildning"

#: src/translate.c.po.c:4250
msgid "Specify name of BASIC ROM image"
msgstr "Ange namn på BASIC-ROM-avbildning"

#: src/translate.c.po.c:4264
msgid "Specify name of character generator ROM image"
msgstr "Ange namn på teckengenerator-ROM-avbildning"

#: src/translate.c.po.c:4279
msgid "Specify index of keymap file (0=sym, 1=symDE, 2=pos)"
msgstr ""
"Ange index för för tangentbordsinställningsfil (0=symbolisk, 1=symbolisk "
"tysk, 2=positionsriktig)"

#: src/translate.c.po.c:4298
msgid "Specify name of symbolic German keymap file"
msgstr "Ange fil för tysk symbolisk tangentbordsemulering"

#: src/translate.c.po.c:4313
msgid "Enable the GEORAM expansion unit"
msgstr "Aktivera GEORAM-expansionsenhet"

#: src/translate.c.po.c:4327
msgid "Disable the GEORAM expansion unit"
msgstr "Inaktivera GEORAM-expansionsenhet"

#: src/translate.c.po.c:4341
msgid "Specify name of GEORAM image"
msgstr "Ange namn på GEORAM-avbildning"

#: src/translate.c.po.c:4355
msgid "<size in KB>"
msgstr "<storlek i KB>"

#: src/translate.c.po.c:4369
msgid "Size of the GEORAM expansion unit"
msgstr "Storlek på GEORAM-expansionsenhet"

#: src/translate.c.po.c:4383
msgid "Enable the PET Ram and Expansion Unit"
msgstr "Aktivera PET Ram- och -expansionsenhet"

#: src/translate.c.po.c:4397
msgid "Disable the PET Ram and Expansion Unit"
msgstr "Inaktivera PET Ram- och -expansionsenhet"

#: src/translate.c.po.c:4411
