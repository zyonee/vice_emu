msgstr "Angiv navn på BASIC ROM-image (lav del)"

#: src/translate_text.c.po.c:3145
msgid "Specify name of BASIC ROM image (higher part)"
msgstr "Angiv navn på BASIC ROM-image (høj del)"

#: src/translate_text.c.po.c:3160
msgid "Specify name of international character generator ROM image"
msgstr "Angiv navn på internationalt tegngenerator-ROM-image"

#: src/translate_text.c.po.c:3175
msgid "Specify name of German character generator ROM image"
msgstr "Angiv navn på tysk tegngenerator-ROM-image"

#: src/translate_text.c.po.c:3190
msgid "Specify name of French character generator ROM image"
msgstr "Angiv navn på fransk tegngenerator-ROM-image"

#: src/translate_text.c.po.c:3205
msgid "Specify name of Swedish character generator ROM image"
msgstr "Angiv navn på svensk tegngenerator-ROM-image"

#: src/translate_text.c.po.c:3220
msgid "Specify name of C64 mode Kernal ROM image"
msgstr "Angiv navn på C64-kernal-ROM-image"

#: src/translate_text.c.po.c:3235
msgid "Specify name of C64 mode BASIC ROM image"
msgstr "Angiv navn på C64-BASIC-ROM-image"

#: src/translate_text.c.po.c:3250
msgid "Enable the OEM userport joystick adapter"
msgstr "Aktivér OEM brugerportjoystickadapter"

#: src/translate_text.c.po.c:3265
msgid "Disable the OEM userport joystick adapter"
msgstr "Deaktivér OEM brugerportjoystickadapter"

#: src/translate_text.c.po.c:3280
msgid "Enable the IEEE488 interface emulation"
msgstr "Aktivér emulering af IEEE488-interface"

#: src/translate_text.c.po.c:3295
msgid "Disable the IEEE488 interface emulation"
msgstr "Deaktivér emulering af IEEE488-interface"

#: src/translate_text.c.po.c:3310
msgid "<revision>"
msgstr "<revision>"

#: src/translate_text.c.po.c:3325
msgid "Patch the Kernal ROM to the specified <revision>"
msgstr "Patch kerne-ROM til den angivne <revision>"

#: src/translate_text.c.po.c:3341
msgid "Enable the $DE** ACIA RS232 interface emulation"
msgstr "Aktivér $DE** ACIA RS232-interface emulering"

#: src/translate_text.c.po.c:3356
msgid "Disable the $DE** ACIA RS232 interface emulation"
msgstr "Deaktivér $DE** ACIA-RS232-interface emulering"

#: src/translate_text.c.po.c:3388
msgid "Specify index of keymap file (0=symbol, 1=positional)"
msgstr ""
"Angiv indeks for tastaturindstillingsfil (0=symbolsk, 1=positionsbestemt)"

#: src/translate_text.c.po.c:3403
msgid "Specify name of symbolic keymap file"
msgstr "Angiv filnavn for symbolsk tastatur"

#: src/translate_text.c.po.c:3418
msgid "Specify name of positional keymap file"
msgstr "Angiv fil for positionsbestemt tastatur"

#: src/translate_text.c.po.c:3434
msgid "Activate 40 column mode"
msgstr "Aktivér 40-kolonners tilstand"

#: src/translate_text.c.po.c:3449
msgid "Activate 80 column mode"
msgstr "Aktivér 80-kolonners tilstand"

#: src/translate_text.c.po.c:3464
msgid "Always switch to C64 mode on reset"
msgstr "Skift altid til C64-tilstand ved reset"

#: src/translate_text.c.po.c:3479
msgid "Always switch to C128 mode on reset"
msgstr "Skift altid til C128-tilstand ved reset"

#: src/translate_text.c.po.c:3494
msgid "Specify name of internal Function ROM image"
msgstr "Angiv navn på internt funktions-ROM-image"

#: src/translate_text.c.po.c:3509
msgid "Specify name of external Function ROM image"
msgstr "Angiv navn på externt funktions-ROM-image"

#: src/translate_text.c.po.c:3524
msgid "Enable the internal Function ROM"
msgstr "Aktivér intern funktions-ROM"

#: src/translate_text.c.po.c:3539
msgid "Disable the internal Function ROM"
msgstr "Deaktivér intern funktions-ROM"

#: src/translate_text.c.po.c:3554
msgid "Enable the external Function ROM"
msgstr "Aktivér extern funktions-ROM"

#: src/translate_text.c.po.c:3569
msgid "Disable the external Function ROM"
msgstr "Deaktivér extern funktions-ROM"

#: src/translate_text.c.po.c:3584
msgid "Use old NTSC sync factor"
msgstr "Anvend gammel NTSC-synkfaktor"

#: src/translate_text.c.po.c:3599
msgid "<model>"
msgstr "<model>"

#: src/translate_text.c.po.c:3614
msgid "Set both CIA models (0 = old 6526, 1 = new 6526A)"
msgstr "Vælg type for begge CIA-kredse (0=gammel 6526, 1=ny 6526A)"

#: src/translate_text.c.po.c:3629
msgid "Set CIA 1 model (0 = old 6526, 1 = new 6526A)"
msgstr "Vælg CIA 1 type (0=gammel 6526, 1=ny 6526A)"

